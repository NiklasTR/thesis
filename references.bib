	title = {{MEK} inhibitor resistance mechanisms and recent developments in combination trials},
	volume = {92},
	issn = {0305-7372, 1532-1967},
	url = {https://www.cancertreatmentreviews.com/article/S0305-7372(20)30175-4/fulltext#secst035},
	doi = {10.1016/j.ctrv.2020.102137},
