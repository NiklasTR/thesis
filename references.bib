@article{kaurWnterLifeWnt2016,
	title = {In the {Wnt}-er of life: {Wnt} signalling in melanoma and ageing},
	volume = {115},
	copyright = {2016 The Author(s)},
	issn = {1532-1827},
	shorttitle = {In the {Wnt}-er of life},
	url = {https://www.nature.com/articles/bjc2016332},
	doi = {10.1038/bjc.2016.332},
	abstract = {Although the clinical landscape of melanoma is improving rapidly, metastatic melanoma remains a deadly disease. Age remains one of the greatest risk factors for melanoma, and patients older than 55 have a much poorer prognosis than younger individuals, even when the data are controlled for grade and stage. The reasons for this disparity have not been fully uncovered, but there is some recent evidence that Wnt signalling may have a role. Wnt signalling is known to have roles both in cancer progression as well as in organismal ageing. In melanoma, the interplay of Wnt signalling pathways is complex, with different members of the Wnt family guiding different aspects of invasion and proliferation. Here, we will briefly review the current literature addressing the roles of different Wnt pathways in melanoma pathogenesis, provide an overview of Wnt signalling during ageing, and discuss the intersection between melanoma and ageing in terms of Wnt signalling.},
