	number = {3},
	journal = {Molecular Cell},
	author = {Yoon, Sang-Oh and Shin, Sejeong and Karreth, Florian A. and Buel, Gwen R. and Jedrychowski, Mark P. and Plas, David R. and Dedhar, Shoukat and Gygi, Steven P. and Roux, Philippe P. and Dephoure, Noah and Blenis, John},
