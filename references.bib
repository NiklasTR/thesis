@article{roperVivoGenomeEditing2017a,
	title = {In vivo genome editing and organoid transplantation models of colorectal cancer},
	volume = {35},
	issn = {1087-0156},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5462879/},
	doi = {10.1038/nbt.3836},
	abstract = {In vivo interrogation of the function of genes implicated in tumorigenesis is limited by the need to generate and cross germline mutant mice. Here we describe approaches to model colorectal cancer (CRC) and metastasis that rely on in situ gene editing and orthotopic organoid transplantation in mice without cancer predisposing mutations. Autochthonous tumor formation is induced by CRISPR–Cas9-based editing of the Apc and Trp53 tumor suppressor genes in colon epithelial cells and by orthotopic transplantation of Apc-edited colon organoids. ApcΔ/ Δ;KrasG12D/+;Trp53Δ/ Δ (AKP) mouse colon organoids and human CRC organoids engraft in the distal colon and metastasize to the liver. Finally, we apply the orthotopic transplantation model to characterize the clonal dynamics of Lgr5+ stem cells and demonstrate sequential activation of an oncogene in established colon adenomas. These experimental systems enable rapid in vivo characterization of cancer-associated genes and reproduce the entire spectrum of tumor progression and metastasis.},
	number = {6},
	urldate = {2023-11-12},
	journal = {Nature biotechnology},
	author = {Roper, Jatin and Tammela, Tuomas and Cetinbas, Naniye Malli and Akkad, Adam and Roghanian, Ali and Rickelt, Steffen and Almeqdadi, Mohammad and Wu, Katherine and Oberli, Matthias and Sánchez-Rivera, Francisco and Park, Yoona and Liang, Xu and Eng, George and Taylor, Martin S. and Azimi, Roxana and Kedrin, Dmitriy and Neupane, Rachit and Beyaz, Semir and Sicinska, Ewa T. and Suarez, Yvelisse and Yoo, James and Chen, Lillian and Zukerberg, Lawrence and Katajisto, Pekka and Deshpande, Vikram and Bass, Adam and Tsichlis, Philip N. and Lees, Jacqueline and Langer, Robert and Hynes, Richard O. and Chen, Jianzhu and Bhutkar, Arjun J. and Jacks, Tyler and Yilmaz, Ömer H.},
